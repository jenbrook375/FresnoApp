    <!-- Credit for logo used as app icon
  https://logomakr.com/5tpFJN , logomakr.com/0eopk2-->

    <application
        android:allowBackup="true"
        android:icon="@drawable/logomakr_5tpfjn"
        android:label="Fresno"
        android:roundIcon="@drawable/logomakr_0eopk2"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
